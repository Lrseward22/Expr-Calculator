#ifndef TOK
#define TOK(ID)
#endif
#ifndef IDENTIFIER
#define IDENTIFIER(ID) TOK(ID)
#endif
#ifndef LITERAL
#define LITERAL(ID) TOK(ID)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(ID, SP) TOK(ID)
#endif
#ifndef KEYWORD
#define KEYWORD(ID, FLAG) TOK(kw_ ## ID)
#endif

TOK(UNKNOWN)
TOK(EOI)

IDENTIFIER(IDENTIFIER)

LITERAL(INTEGER_LITERAL)

PUNCTUATOR(PLUS,                "+")
PUNCTUATOR(MINUS,               "-")
PUNCTUATOR(STAR,                "*")
PUNCTUATOR(SEMI,                ";")
PUNCTUATOR(PLUSEQUAL,           "+=")
PUNCTUATOR(MINUSEQUAL,          "-=")
PUNCTUATOR(EQUAL,               "=")
PUNCTUATOR(L_PAREN,             "(")
PUNCTUATOR(R_PAREN,             ")")
// ...

KEYWORD(read                        , KEYALL)
// ...

#undef KEYWORD
#undef PUNCTUATOR
#undef TOK
